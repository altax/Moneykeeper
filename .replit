entrypoint = "README.md"
modules = ["nodejs-20", "web"]

[nix]
channel = "stable-25_05"

[deployment]
deploymentTarget = "cloudrun"
build = ["sh", "-c", "npm run expo:static:build && npm run server:build"]
run = ["npm", "run", "server:prod"]

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 8081
externalPort = 80

[[ports]]
localPort = 8082
externalPort = 3000

[[ports]]
localPort = 33855
externalPort = 3003

[[ports]]
localPort = 38751
externalPort = 4200

[[ports]]
localPort = 39049
externalPort = 3002

[[ports]]
localPort = 39789
externalPort = 5173

[[ports]]
localPort = 42913
externalPort = 3001

[env]
PORT = "5000"

[agent]
stack = "EXPO"
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start dev servers"

[[workflows.workflow]]
name = "Start dev servers"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run all:dev"
waitForPort = 8081
ensurePreviewReachable = "/status"

